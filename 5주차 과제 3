    # return nums
    # return [n for n in range(1, num + 1)]
    # 위 4줄을 한줄에 쓰는법

    # nums = []
    # for n in range(1, num + 1):
    #     nums.append(n+1)
    #     nums = nums + [n]
    # return nums
    return [n+1 for n in range(num)]

    # a = [1,2,3]
