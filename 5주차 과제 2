def average(nums):
    # return sum(sums)/len(nums)
    total = 0
    count = 0
    for n in nums:
        total += n
        count += 1
    return total / count


def main():
    nums = [80, 90, 100, 90]
    print(f"주어진 리스트는{nums}")
    print("주어진 리스트는", nums, "입니다.")
    print(average(nums))


if __name__ == '__main__':
    main()
